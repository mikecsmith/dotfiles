{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "codeLens.enable": true,
  "diagnostics": {
    "globals": [
      "vim"
    ],
    "disable": [
      "close-non-object",
      "incomplete-signature-doc",
      "missing-global-doc",
      "missing-local-export-doc"
    ],
    "groupFileStatus": {
      "ambiguity": "Any",
      "await": "Any",
      "duplicate": "Any",
      "global": "Any",
      "luadoc": "Any",
      "redefined": "Any",
      "strict": "Any",
      "type-check": "Any",
      "unbalanced": "Any",
      "unused": "Any"
    },
    "ignoredFiles": "Opened",
    "libraryFiles": "Opened"
  },
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "maxPreload": 1600,
    "preloadFileSize": 1000,
    "ignoreDir": [],
    "checkThirdParty": false
  },
  "typeFormat": {
    "config": {
      "format_line": "false"
    }
  },
  "type": {
    "castNumberToInteger": false
  },
  "doc": {
    "privateName": [
      "_*"
    ]
  }
}
